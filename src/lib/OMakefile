
LIB_MODULES[] =
    dibrawi_Yaboon_PolyComp
    dibrawi_std
    dibrawi_templating
    dibrawi

LIB_NAME = dibrawi

.DEFAULT: $(OCamlLibrary $(LIB_NAME), $(LIB_MODULES))

install_library: dibrawi.cma dibrawi.cmxa META
    ocamlfind install dibrawi META *.cm[iox] dibrawi.cma dibrawi.cmxa dibrawi.a
uninstall_library:
    ocamlfind remove sebib
reinstall_library: uninstall_library install_library

